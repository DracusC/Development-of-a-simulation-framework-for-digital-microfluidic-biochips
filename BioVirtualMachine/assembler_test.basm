#Testing program

.text
DEF elecToSet 0
DEF elecToClr 1
DEF maxX 2
DEF minX 3


init:
  LI elecToSet 100;
  LI elecToClr 101;
  LI minX 100;
  LI maxX 124;

start:
  SETEL elecToSet;
  TICK;
  CLREL elecToClr;
  TICK;

  ADDI elecToClr elecToSet 0; #Copy
  ADDI elecToSet elecToSet 1;

  BGE backwards elecToSet maxX;
  JI start;

backwards:
  SETEL elecToSet;
  TICK;
  CLREL elecToClr;
  TICK;

  ADDI elecToClr elecToSet 0; #Copy
  SUBI elecToSet elecToSet 1;

  BLE start elecToSet minX;
  JI backwards;
